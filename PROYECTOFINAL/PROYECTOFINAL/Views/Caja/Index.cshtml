
@{
    ViewBag.Title = "ListarMovimientosCaja";
}

@using PROYECTOFINAL.Models
@model List<movimientosmodel>

<h2>Listar movimientos de la caja</h2>

<form action="../Caja/MostrarMovDia" method="POST">

    @Html.Label("Ingrese dia: ")
